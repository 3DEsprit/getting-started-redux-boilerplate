{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": { 
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "eol-last": 0,
    "quotes": [2, "single"]
  }
}
